[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
	pl = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
[push]
	default = simple
[color]
	status = true
	ui = true
[core]
	quotepath = false
	editor = emacsclient 
